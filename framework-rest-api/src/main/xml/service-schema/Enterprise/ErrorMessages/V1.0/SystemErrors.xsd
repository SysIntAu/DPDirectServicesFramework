<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:errcore="http://www.dpdirect.org/Namespace/Enterprise/ErrorMessages/V1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.dpdirect.org/Namespace/Enterprise/ErrorMessages/V1.0" elementFormDefault="qualified" version="1.0" xml:lang="en">
  <xs:include schemaLocation="ErrorCore.xsd"/>
  <xs:complexType name="systemErrorType">
    <xs:sequence>
      <xs:element ref="errcore:Code"/>
      <xs:element ref="errcore:Description"/>
      <xs:element ref="errcore:SubCode" minOccurs="0"/>
      <xs:element ref="errcore:SubDescription" minOccurs="0"/>
      <xs:element ref="errcore:Location" minOccurs="0"/>
      <xs:element ref="errcore:MessageOrigin"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="systemErrorsType">
    <xs:sequence>
      <xs:element ref="errcore:SystemError" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="SystemError" type="errcore:systemErrorType"/>
  <xs:element name="SystemErrors" type="errcore:systemErrorsType"/>
</xs:schema>
